{#
/**
 * @file
 * Default theme implementation to display a single page.
 *
 * The doctype, html, head and body tags are not in this template. Instead they
 * can be found in the html.html.twig template in this directory.
 *
 * Available variables:
 *
 * General utility variables:
 * - base_path: The base URL path of the Drupal installation. Will usually be
 *   "/" unless you have installed Drupal in a sub-directory.
 * - is_front: A flag indicating if the current page is the front page.
 * - logged_in: A flag indicating if the user is registered and signed in.
 * - is_admin: A flag indicating if the user has permission to access
 *   administration pages.
 *
 * Site identity:
 * - front_page: The URL of the front page. Use this instead of base_path when
 *   linking to the front page. This includes the language domain or prefix.
 *
 * Navigation:
 * - breadcrumb: The breadcrumb trail for the current page.
 *
 * Page content (in order of occurrence in the default page.html.twig):
 * - title_prefix: Additional output populated by modules, intended to be
 *   displayed in front of the main title tag that appears in the template.
 * - title: The page title, for use in the actual content.
 * - title_suffix: Additional output populated by modules, intended to be
 *   displayed after the main title tag that appears in the template.
 * - messages: Status and error messages. Should be displayed prominently.
 * - tabs: Tabs linking to any sub-pages beneath the current page (e.g., the
 *   view and edit tabs when displaying a node).
 * - action_links: Actions local to the page, such as "Add menu" on the menu
 *   administration interface.
 * - node: Fully loaded node, if there is an automatically-loaded node
 *   associated with the page and the node ID is the second argument in the
 *   page's path (e.g. node/12345 and node/12345/revisions, but not
 *   comment/reply/12345).
 *
 * Regions:
 * - page.header: Items for the header region.
 * - page.navigation: Items for the navigation region.
 * - page.navigation_collapsible: Items for the navigation (collapsible) region.
 * - page.highlighted: Items for the highlighted content region.
 * - page.help: Dynamic help text, mostly for admin pages.
 * - page.content: The main content of the current page.
 * - page.sidebar_first: Items for the first sidebar.
 * - page.sidebar_second: Items for the second sidebar.
 * - page.footer: Items for the footer region.
 *
 * @ingroup templates
 *
 * @see template_preprocess_page()
 * @see html.html.twig
 */
#}
{% set title_background = node.field_title_background_color.color %}
{% set show_give = site_settings.utility_bar.give_link.field_display_give_link %}
{% set give_text = site_settings.utility_bar.give_link.field_link_text %}
{% set give_url = site_settings.utility_bar.give_link.field_url.uri %}
{% set status_code = site_settings.sist_settings.statuspage_widget %}
{% set show_highlighted = site_settings.utility_bar.highlighted_link ? 1 : 0 %}
{% set highlighted_text = site_settings.utility_bar.highlighted_link.title %}
{% set highlighted_url = file_url(site_settings.utility_bar.highlighted_link.uri) %}
{%
  set title_classes = [
    'content-header',
    title_background ? 'has-background background-color-' ~ title_background,
    page.secondary_navigation ? 'has-secondary-navigation',
  ]
%}

{% set smcount = 0 %}
{% for item in site_settings.footer.social_media %}
  {% set smcount = smcount + 1 %}
{% endfor %}

{% if smcount == 1 %}
  {% set smcolumns = 1 %}
{% elseif smcount == 2 or smcount == 4 %}
  {% set smcolumns = 2 %}
{% else %}
  {% set smcolumns = 3 %}
{% endif %}

{% set flcount = 0 %}
{% for item in site_settings.footer.footer_links %}
  {% set flcount = flcount + 1 %}
{% endfor %}

{% if flcount == 1 %}
  {% set flcolumns = 6 %}
{% elseif flcount == 2 %}
  {% set flcolumns = 3 %}
{% elseif flcount == 3 %}
  {% set flcolumns = 2 %}
{% endif %}

{% if node.field_overlay_position.value == 1 %}
  {% set overlay_position = 'overlay-1' %}
{% elseif node.field_overlay_position.value == 2 %}
  {% set overlay_position = 'overlay-2' %}
{% elseif node.field_overlay_position.value == 3 %}
  {% set overlay_position = 'overlay-3' %}
{% elseif node.field_overlay_position.value == 4 %}
  {% set overlay_position = 'overlay-4' %}
{% endif %}

{% set container = theme.settings.fluid_container ? 'container-fluid' : 'container' %}
{# Navbar #}
{% if page.navigation or page.navigation_collapsible %}
  {% block navbar %}
    {%
      set navbar_classes = [
        'navbar',
        theme.settings.navbar_inverse ? 'navbar-inverse' : 'navbar-default',
        theme.settings.navbar_position ? 'navbar-' ~ theme.settings.navbar_position|clean_class : container,
        node.field_hero_image.entity.fileuri ? 'with-hero',
        node.field_hero_image_media.entity.field_media_image.entity.uri.value ? 'with-hero',
      ]
    %}
    <header{{ navbar_attributes.addClass(navbar_classes) }} id="navbar">
      <a href="#main-content" class="visually-hidden focusable skip-link">
        {{ 'Skip to main content'|t }}
      </a>
      {% if page.alert %}
        {{ page.alert }}
      {% endif %}
      <div id="eyebrow">
        <div class="inner">
          <div class="items">
            <div class="item">
              <a href="https://www.ucsb.edu/" class="edu-link">University of California, Santa Barbara</a>
            </div>
            <div class="item">
              <div class="inner">
                {% if show_highlighted %}
                    <div class="highlighted">
                      <a href="{{ highlighted_url }}">
                      {{ highlighted_text }}
                      </a>
                    </div>
                {% endif %}
                {% if show_give == 1 %}
                  <div class="give"><a href="{{ give_url }}">{{ give_text }}</a></div>
                {% endif %}
                <div class="quick-links"><a href="#">Quick Links</a>
                  <svg viewBox="0 0 24 24" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <path d="M12,12.5857864 L8.70710678,9.29289322 C8.31658249,8.90236893 7.68341751,8.90236893 7.29289322,9.29289322 C6.90236893,9.68341751 6.90236893,10.3165825 7.29289322,10.7071068 L11.2928932,14.7071068 C11.6834175,15.0976311 12.3165825,15.0976311 12.7071068,14.7071068 L16.7071068,10.7071068 C17.0976311,10.3165825 17.0976311,9.68341751 16.7071068,9.29289322 C16.3165825,8.90236893 15.6834175,8.90236893 15.2928932,9.29289322 L12,12.5857864 Z"></path>
                  </svg>
                </div>
                <div class="search">
                  <span class="toggle-button">
                    <svg class="mag" viewBox="0 0 24 24" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                      <path d="M15.2397415,16.6539551 C14.1023202,17.4996184 12.6929286,18 11.1666667,18 C7.39272088,18 4.33333333,14.9406125 4.33333333,11.1666667 C4.33333333,7.39272088 7.39272088,4.33333333 11.1666667,4.33333333 C14.9406125,4.33333333 18,7.39272088 18,11.1666667 C18,12.6929286 17.4996184,14.1023202 16.6539551,15.2397415 L20.2071068,18.7928932 C20.5976311,19.1834175 20.5976311,19.8165825 20.2071068,20.2071068 C19.8165825,20.5976311 19.1834175,20.5976311 18.7928932,20.2071068 L15.2397415,16.6539551 Z M11.1666667,16 C13.836043,16 16,13.836043 16,11.1666667 C16,8.49729038 13.836043,6.33333333 11.1666667,6.33333333 C8.49729038,6.33333333 6.33333333,8.49729038 6.33333333,11.1666667 C6.33333333,13.836043 8.49729038,16 11.1666667,16 Z"></path>
                    </svg>
                    <svg class="arrow" viewBox="0 0 24 24" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                      <path d="M12,12.5857864 L8.70710678,9.29289322 C8.31658249,8.90236893 7.68341751,8.90236893 7.29289322,9.29289322 C6.90236893,9.68341751 6.90236893,10.3165825 7.29289322,10.7071068 L11.2928932,14.7071068 C11.6834175,15.0976311 12.3165825,15.0976311 12.7071068,14.7071068 L16.7071068,10.7071068 C17.0976311,10.3165825 17.0976311,9.68341751 16.7071068,9.29289322 C16.3165825,8.90236893 15.6834175,8.90236893 15.2928932,9.29289322 L12,12.5857864 Z"></path>
                    </svg>
                  </span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>    
      {% if not navbar_attributes.hasClass(container) %}
        <div class="{{ container }}">
      {% endif %}
      <div class="navbar-header">
        {{ page.navigation }}
        {# .btn-navbar is used as the toggle for collapsed navbar content #}
        {% if page.navigation_collapsible %}
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
            <span class="sr-only">{{ 'Toggle navigation'|t }}</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        {% endif %}
      </div>

      {# Navigation (collapsible) #}
      {% if page.navigation_collapsible %}
        <div id="navbar-collapse" class="navbar-collapse collapse">
          {{ page.navigation_collapsible }}
          {{ page.navigation_quicklinks }}
          {% if show_give == 1 or show_highlighted == 1 %}
            <div class="utility-bar">
              <ul class="menu nav">
                {% if show_give == 1 %}
                <li><a href="{{ give_url }}">{{ give_text }}</a></li>
                {% endif %}
                {% if show_highlighted == 1 %}
                <li class="highlighted"><a href="{{ highlighted_url }}">{{ highlighted_text }}</a></li>
                {% endif %}
              </ul>
            </div>
          {% endif %}
        </div>
      {% endif %}
      {% if not navbar_attributes.hasClass(container) %}
        </div>
      {% endif %}
    </header>
  {% endblock %}
{% endif %}
{# SIST Settings #}
{% if status_code %}
  {{ status_code|raw }}
{% endif %}
{# Main #}
{% block main %}
  <main>
    <div class="main-container {{ container }} js-quickedit-main-content">
      <div class="row">

        {# Secondary navigation #}
        {% if page.secondary_navigation %}
          {% block secondary_navigation %}
            <div class="secondary-navigation">{{ page.secondary_navigation }}</div>
          {% endblock %}
        {% endif %}

          {% block hero %}
        {% if node.field_hero_image.entity.fileuri %}

          {% if node.field_hero_image.width > 1680 %}
            {% set image_height = node.field_hero_image.height / 2 %}
          {% else %}
            {% set image_height = node.field_hero_image.height %}
          {% endif %}
			{%
              set hero_classes = [
              'hero-image',
              image_height ? 'height-' ~ image_height,
              overlay_position ? 'overlay ' ~ overlay_position,
            ]
            %}
            {% set overlay_width = ' col-md-' ~ node.field_overlay_width.value %}
            {% if node.field_overlay_background_color.color is not null %}
              {% set overlay_color = ' has-background background-color-' ~ node.field_overlay_background_color.color %}
            {% endif %}
          <div class="hero-image height-{{ image_height }}" style="background-image: url({{ file_url(node.field_hero_image.entity.fileuri) }});">
          </div>
              {% if node.field_overlay %}
                <div class="inner">
                  <div class="overlay-content{{ overlay_color }}{{ overlay_width }}">
                    {% autoescape false %}
                      {{ node.field_overlay.value }}
                    {% endautoescape %}
                  </div>
                </div>
              {% endif %}
            </div>
		  
        {% endif %}
          

        {% if node.field_hero_image_media.entity.field_media_image.entity.uri.value %}
        
          {% set hero_image_width = node.field_hero_image_media.entity.field_media_image.width %}
          {% set hero_image_height = node.field_hero_image_media.entity.field_media_image.height %}

          {% if hero_image_width > 1680 %}
            {% set image_height = hero_image_height / 2 %}
          {% else %}
            {% set image_height = hero_image_height %}
          {% endif %} 

          {% if image_height >= 1920 %}
            {% set image_height = 1920 %}
          {% elseif image_height >= 1560 and image_height < 1920 %}
            {% set image_height = 1560 %}
          {% elseif image_height >= 1440 and image_height < 1560 %}
            {% set image_height = 1440 %}
          {% elseif image_height >= 1200 and image_height < 1440 %}
            {% set image_height = 1200 %}
          {% elseif image_height >= 960 and image_height < 1200 %}
            {% set image_height = 960 %}
          {% elseif image_height >= 780 and image_height < 960 %}
            {% set image_height = 780 %}
          {% elseif image_height >= 720 and image_height < 780 %}
            {% set image_height = 720 %}
          {% elseif image_height >= 600 and image_height < 720 %}
            {% set image_height = 600 %}
          {% elseif image_height >= 500 and image_height < 600 %}
            {% set image_height = 500 %}
          {% elseif image_height >= 480 and image_height < 500 %}
            {% set image_height = 480 %}
          {% else %}
            {% set image_height = 480 %}
          {% endif %} 
	
            {%
              set hero_classes = [
              'hero-image',
              image_height ? 'height-' ~ image_height,
              overlay_position ? 'overlay ' ~ overlay_position,
            ]
            %}
            {% set overlay_width = ' col-md-' ~ node.field_overlay_width.value %}
            {% if node.field_overlay_background_color.color is not null %}
              {% set overlay_color = ' has-background background-color-' ~ node.field_overlay_background_color.color %}
            {% endif %}
          <div{{ attributes.addClass(hero_classes) }} style="background-image: url({{ file_url(node.field_hero_image_media.entity.field_media_image.entity.uri.value) }});">
		      {% if node.field_overlay %}
                <div class="inner">
                  <div class="overlay-content{{ overlay_color }}{{ overlay_width }}">
                    {% autoescape false %}
                      {{ node.field_overlay.value }}
                    {% endautoescape %}
                  </div>
                </div>
              {% endif %}
          </div>

        {% endif %}
          {% endblock %}
		  
        {% if node.field_hero_video.value %}
          <div class="hero-video">
            {% autoescape false %}
              {{ node.field_hero_video.value }}
            {% endautoescape %}
          </div>
        {% endif %}

        {# Header #}
        {% if page.header %}
          {% block header %}
            <div{{ header_attributes.addClass(title_classes) }}>
              {{ page.header }}
            </div>
            
          {% endblock %}
        {% endif %}

        {# Sidebar First #}
        {% if page.sidebar_first %}
          {% block sidebar_first %}
            <aside class="col-sm-3">
              {{ page.sidebar_first }}
            </aside>
          {% endblock %}
        {% endif %}

        {# Content #}
        {%
          set content_classes = [
            page.sidebar_first and page.sidebar_second ? 'col-sm-6',
            page.sidebar_first and page.sidebar_second is empty ? 'col-sm-9',
            page.sidebar_second and page.sidebar_first is empty ? 'col-sm-9',
            page.sidebar_first is empty and page.sidebar_second is empty ? 'col-sm-12',
          'main-content-wrapper',
          ]
        %}
        <div{{ content_attributes.addClass(content_classes) }}>

          {# Highlighted #}
          {% if page.highlighted %}
            {% block highlighted %}
              <div class="highlighted">{{ page.highlighted }}</div>
            {% endblock %}
          {% endif %}

          {# Breadcrumbs #}
          {% if breadcrumb %}
            {% block breadcrumb %}
              {{ breadcrumb }}
            {% endblock %}
          {% endif %}

          {# Action Links #}
          {% if action_links %}
            {% block action_links %}
              <ul class="action-links">{{ action_links }}</ul>
            {% endblock %}
          {% endif %}

          {# Help #}
          {% if page.help %}
            {% block help %}
              {{ page.help }}
            {% endblock %}
          {% endif %}

          {% if node_type == 'blog_post' %}
        </div>
        <div class="article-header">
          {% if node.field_image.entity.fileuri %}
            <div class="article-image">
              <div class="inner">
                <img src="{{ file_url(node.field_image.entity.fileuri) }}"/>
              </div>
            </div>
          {% endif %}
          <div class="article-teaser">
            <div class="inner">
                  <span class="serif">
                    {{ node_teaser }}
                  </span>
            </div>
          </div>
        </div>
        <div{{ content_attributes.addClass(content_classes) }}>
          {% endif %}
		  
          {# Content #}
          {% block content %}
            <div class="main-forum-content">              
              <a id="main-content"></a>
              {{ page.content }}
            </div>
          {% endblock %}
        </div>

        {# Sidebar Second #}
        {% if page.sidebar_second %}
          {% block sidebar_second %}
            <aside class="col-sm-3">
              {{ page.sidebar_second }}
            </aside>
          {% endblock %}
        {% endif %}
      </div>
    </div>
  </main>
{% endblock %}

{% if is_blog %}
  <div class="topics-list">
    <div class="inner">
      <h2>More {{ blog_title }} topics</h2>
      {% if is_topics == 1 %}
        {{ all_topics_list }}
      {% else %}
        {{ all_topics_list }}
      {% endif %}
    </div>
  </div>
{% endif %}

