    </div>
    
    
    <div class="col-12 col-md-9">
      <div class="field field--name-field-blades">
        <div class="paragraph paragraph--type--blade paragraph--view-mode--default">
          <div class="inner">
            {{ page.content }}
          </div>
        </div>
      </div>
    </div>
  </div>
</main>

{% if page.sidebar_first %}
  <aside role="complementary">
    {{ page.sidebar_first }}
  </aside>
{% endif %}

{% if page.sidebar_second %}
  <aside role="complementary">
    {{ page.sidebar_second }}
  </aside>
{% endif %}

