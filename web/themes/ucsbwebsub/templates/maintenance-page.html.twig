{#
/**
 * @file
 * Default theme implementation to display a single Drupal page while offline.
 *
 * All available variables are mirrored in page.html.twig.
 * Some may be blank but they are provided for consistency.
 *
 * @see template_preprocess_maintenance_page()
 *
 * @ingroup themeable
 */
#}

{% set show_give = site_settings.utility_bar.give_link.field_display_give_link %}
{% set give_text = site_settings.utility_bar.give_link.field_link_text %}
{% set give_url = site_settings.utility_bar.give_link.field_url.uri %}
{%
  set title_classes = [
    'content-header',
  ]
%}

{% set smcount = 0 %}
{% for item in site_settings.footer.social_media %}
  {% set smcount = smcount + 1 %}
{% endfor %}

{% if smcount == 1 %}
  {% set smcolumns = 1 %}
{% elseif smcount == 2 or smcount == 4 %}
  {% set smcolumns = 2 %}
{% else %}
  {% set smcolumns = 3 %}
{% endif %}

{% set flcount = 0 %}
{% for item in site_settings.footer.footer_links %}
  {% set flcount = flcount + 1 %}
{% endfor %}

{% if flcount == 1 %}
  {% set flcolumns = 6 %}
{% elseif flcount == 2 %}
  {% set flcolumns = 3 %}
{% elseif flcount == 3 %}
  {% set flcolumns = 2 %}
{% endif %}

{% set container = theme.settings.fluid_container ? 'container-fluid' : 'container' %}
{# Navbar #}
